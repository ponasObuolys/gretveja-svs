{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 1024
    }
  },
  "env": {
    "SUPABASE_URL": "https://krddmfggwygganqronyl.supabase.co",
    "SUPABASE_KEY": "@supabase_key",
    "NODE_ENV": "production"
  }
}
